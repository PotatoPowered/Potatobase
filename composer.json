{
  "name": "potatopowered/potatobase",
  "description": "Potatobase repository will be an extension of the Phinx migration tool that allows us to define standards and other database migration specific standards.",
  "type": "cakephp-plugin",
  "keywords": [
    "potato",
    "potatobase",
    "migrations",
    "CakePHP"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Blake Sutton",
      "email": "blake@potatopowered.net",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": ">=3.0",
    "cakephp/bake": ">=1.2.6",
    "cakephp/migrations": ">=1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "cakephp/cakephp-codesniffer": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Potatobase\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Potatobase\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
